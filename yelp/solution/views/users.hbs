<h3>Contacts</h3>
<table class="table table-hover">
  <thead>
    <td><b>Name</b></td>
    <td><b>Email</b></td>
    <td><b>Address</b></td>
    <td><b>Reviews</b></td>
    <td><b>Friends</b></td>
    <td><b>Links</b></td>
  </thead>
  <tbody>
    {{#each users}}
    <tr>
      <td>{{displayName}}</td>
      <td>{{username}}</td>
      <td>{{address}}</td>
      <td>{{reviews}}</td>
      <td>{{friends}}</td>
      <td>
        <a href="/profile/{{id}}">Profile</a>
        <form action="/follow/{{id}}" method="post">
          <input type="submit" value="Follow">
        </form>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
